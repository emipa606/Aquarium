      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>MeatRaw</li>
      </categories>
      <disallowedThingDefs>
        <li>AQCollagen</li>
        <li>Meat_Megaspider</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
    <products>
