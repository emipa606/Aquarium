<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/TraderKindDef[defName="Caravan_Neolithic_ShamanMerchant"]/stockGenerators</xpath>
        <value>
          <li Class="StockGenerator_Category">
            <categoryDef>AQTropicalFish</categoryDef>
            <thingDefCountRange>
              <min>-1</min>
              <max>3</max>
            </thingDefCountRange>
            <totalPriceRange>
              <min>50</min>
              <max>100</max>
            </totalPriceRange>
          </li>
          <li Class="StockGenerator_SingleDef">
            <thingDef>AQFishFood</thingDef>
            <countRange>100~300</countRange>
          </li>
          <li Class="StockGenerator_SingleDef">
            <thingDef>AQGlassBowl</thingDef>
            <countRange>-2~2</countRange>
          </li>
        </value>
      </li>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/TraderKindDef[defName="Caravan_Outlander_Exotic"]/stockGenerators</xpath>
        <value>
          <li Class="StockGenerator_Category">
            <categoryDef>AQTropicalFish</categoryDef>
            <thingDefCountRange>
              <min>-1</min>
              <max>5</max>
            </thingDefCountRange>
            <totalPriceRange>
              <min>50</min>
              <max>120</max>
            </totalPriceRange>
          </li>
          <li Class="StockGenerator_SingleDef">
            <thingDef>AQFishFood</thingDef>
            <countRange>200~500</countRange>
          </li>
          <li Class="StockGenerator_SingleDef">
            <thingDef>AQGlassBowl</thingDef>
            <countRange>-1~3</countRange>
          </li>
        </value>
      </li>
      <li Class="PatchOperationAdd">
        <xpath>/Defs/TraderKindDef[defName="Orbital_Exotic"]/stockGenerators</xpath>
        <value>
          <li Class="StockGenerator_Category">
            <categoryDef>AQTropicalFish</categoryDef>
            <thingDefCountRange>
              <min>-1</min>
              <max>7</max>
            </thingDefCountRange>
            <totalPriceRange>
              <min>100</min>
              <max>150</max>
            </totalPriceRange>
          </li>
          <li Class="StockGenerator_SingleDef">
            <thingDef>AQFishFood</thingDef>
            <countRange>300~700</countRange>
          </li>
          <li Class="StockGenerator_SingleDef">
            <thingDef>AQGlassBowl</thingDef>
            <countRange>-1~5</countRange>
          </li>
          <li Class="StockGenerator_SingleDef">
            <thingDef>AQGlassPanel</thingDef>
            <countRange>-2~4</countRange>
          </li>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>
